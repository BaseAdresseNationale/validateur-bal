{
  "name": "@ban-team/validateur-bal",
  "version": "2.21.0",
  "description": "Validateur de référence pour le format BAL",
  "repository": "https://github.com/BaseAdresseNationale/validateur-bal",
  "author": "Équipe Adresse <adresse@data.gouv.fr>",
  "contributors": [
    "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
    "Théophile Merlière <theophile.merliere@data.gouv.fr>",
    "Guillaume Fay <guillaume.fay@beta.gouv.fr>",
    "Fabien Tafforeau <fabien.tafforeau@beta.gouv.fr>"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "bin",
    "lib",
    "browser",
    "minicog.json",
    "*.js"
  ],
  "bin": {
    "bal": "bin/bal"
  },
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint --fix lib scripts",
    "format": "yarn run prettier --write \"lib/**/*.js\" \"scripts/**/*.js\"",
    "build-minicog": "node scripts/build-minicog",
    "build-bins": "pkg --targets node16-linux-x64,node16-macos-x64,node16-win-x64 --output bal .",
    "transpile": "rm -rf browser && babel dist --out-dir browser",
    "test": "nyc ava",
    "prepublishOnly": "yarn build-minicog && yarn build && yarn transpile"
  },
  "dependencies": {
    "@ban-team/shared-data": "^1.4.0",
    "@etalab/project-legal": "^0.6.0",
    "blob-to-buffer": "^1.2.9",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "chardet": "^1.4.0",
    "date-fns": "^2.29.3",
    "file-type": "^12.4.2",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.2",
    "uuidv4": "^6.2.13",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@etalab/decoupage-administratif": "^4.0.0",
    "@types/node": "^22.12.0",
    "@types/papaparse": "^5.3.15",
    "ava": "^4.3.3",
    "codecov": "^3.8.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^10.1.0",
    "nyc": "^15.1.0",
    "pkg": "^5.8.0",
    "prettier": "^3.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "browser": {
    "./lib/index.js": "./browser/index.js",
    "./browser/validate/parse/index.js": "./browser/validate/parse/blob.js"
  },
  "browserslist": "> 0.5% in FR, last 2 versions, Firefox ESR, not ie 11, not dead",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "ignore": [
      "**/__tests__",
      "**/commands"
    ]
  }
}
